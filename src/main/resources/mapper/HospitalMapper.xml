<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.leepon.mapper.HospitalMapper">

   <select id="getHospitalList" resultType="cn.leepon.po.THospital">
        
        SELECT t.hospital_id hospitalId,
               t.hospital_name hospitalName
        FROM t_hospital t
   
   </select>
  
</mapper>